def abc(n):
    if n==1:
        return 2
    b=abc(n-1)
    print("Hai")
    return b+1

print(abc(5))

-------------------------------------------------------

def ram(n):
    if(n==4):
        return
    print(n+10)
    ram(n+1)
    print(n)
    
ram(1)

---------------------------------------------------------
def abc(n,c):
    if n==4:
        return 1
    c=c+1
    b=abc(n+1,c)
    print(c,end="")
    return b*2

print(abc(1,1))

---------------------------------------------------------
def raju(a):
    if(a==1):
        return
    a=a-1
    raju(a)
    print("hai")
    raju(a)

raju(5)

-----------------------------------------------------------
